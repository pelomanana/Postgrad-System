<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <link rel="stylesheet" type="text/css" href="css/viewDocumentation.css">
        <title>Postgraduate Application System</title> 
    <body>
        <h1>Staff Portal</h1>
        <div id="content">
            <form id="staffPortal" method="post">
                <table id="documents">
                <tr class="header">
                    <th>ID</th>
                    <th>App. ID</th>
                    <th>Student ID</th>
                    <th>Student No</th>
                    <th>Programme</th>
                    <th>Course</th>
                    <th>Mark I</th>
                    <th>Mark II</th>
                    <th>Mark III</th>
                    <th>Mark IV</th>
                    <th>Mark V</th>
                    <th>Mark VI</th>
                    <th>Motivation</th>
                    <th>Topic</th>
                    <th>Supervisor</th>
                    <th>Proposal</th>
                    <th>Degree</th>
                    <th>App. Status</th>
                </tr>
                <tr>
                    <td class="lightBlue">[id]</td>
                    <td class="lightBlue">[app id]</td>
                    <td class="lightBlue">[student id]</td>
                    <td class="lightBlue">[student no]</td>
                    <td class="lightBlue">[programme]</td>
                    <td class="lightBlue">[course]</td>
                    <td class="lightBlue">[mark i]</td>
                    <td class="lightBlue">[mark ii]</td>
                    <td class="lightBlue">[mark iii]</td>
                    <td class="lightBlue">[mark iv]</td>
                    <td class="lightBlue">[mark v]</td>
                    <td class="lightBlue">[mark vi]</td>
                    <td class="lightBlue">[motivation]</td>
                    <td class="lightBlue">[topic]</td>
                    <td class="lightBlue">[supervisor]</td>
                    <td class="lightBlue">[proposal]</td>
                    <td class="lightBlue">[degree]</td>
                   <td class="lightBlue">[status]</td>
                </tr>
            </table>
           <button type="logout">Logout</button>
            </form>
        </div>
    </body>
</html>
